<% include layouts/header %>


<h1>coupens</h1>

<div class="row">
    <a href="/admin/users/api/coupens/add" class="col l12 btn">add coupens</a>


    <div class="collection">
        <div class="row">
            <% data.forEach(shit=>{%>
            <div class="collection-item avatar col l12 m12 s12">
                <img src="<%=shit.img%>" alt="" class="left">
                <span class="title">Title</span>
                <div>
                    title: <%=shit.title%>
                    <sub class="red-text">by:<%=shit.author%></sub> <br>
                    description: <%= shit.description%> <br>
                    number: <%= shit.number%> <br>
                    expire date: <%= shit.expire%> <br>
                    catogery: <%= shit.forW%> <br>
                    link: <%=shit.link%>
                </div>
                <a href="" class="secondary-content"><i class="delete-item-coupens material-icons black-text" data-id="<%=shit.id%>">close</i></a>
            </div>
            <%}) %>
        </div>
    </div>



</div>


<% include layouts/footer %>